
\*Factorial of the number*\
#include<stdio.h>
#include<stdio.h>
int factorial(int n)
void main()
{
    int n,f=1;
    printf("Enter the number:");
    scanf("%d",&n);
    f=factorial(n);
    printf("Factorial of the given number is %d",f);
    getch();
}
int factorial(int n)
{
    int f;
    if(n==1)
    return 1;
    else
    f=n*factorial(n-1);
    return f;
    
     
    
}
    
